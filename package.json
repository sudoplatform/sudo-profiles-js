{
  "name": "@sudoplatform/sudo-profiles",
  "version": "0.0.0",
  "author": "Anonyome Labs, Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/sudoplatform/sudo-profiles-js.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "prepare": "husky install",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint \"{bin,test,src}/**/*.{js,ts,tsx}\"",
    "lint:prettier": "prettier -l \"{bin,test,src}/**/*.{js,json,jsx,ts,tsx}\"",
    "codegen": "graphql-codegen -c codegen.yml",
    "verify": "yarn audit-with-suppressions && yarn lint && yarn unit-test",
    "test": "yarn unit-test",
    "unit-test": "/bin/bash -c 'jest test/unit'",
    "integration-test": "/bin/bash -c 'jest --runInBand test/integration' ",
    "clean": "rm -rf esm lib docs types",
    "build": "yarn codegen && yarn verify && yarn build:transpile && yarn build:docs",
    "build:transpile": "concurrently \"yarn build:transpile:declarations\" \"yarn build:transpile:cjs\" \"yarn build:transpile:esm\"",
    "build:transpile:cjs": "tsc --project tsconfig.commonjs.json",
    "build:transpile:esm": "tsc",
    "build:transpile:declarations": "tsc --project tsconfig.declarations.json",
    "build:docs": "typedoc --excludePrivate --out docs src/index.ts"
  },
  "main": "./cjs/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.js",
      "require": "./cjs/index.js",
      "default": "./lib/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "cjs",
    "lib",
    "types"
  ],
  "dependencies": {
    "@sudoplatform/sudo-web-crypto-provider": "^8.0.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "aws-appsync": "^4.1.9",
    "aws-sdk": "^2.1330.0",
    "fp-ts": "^2.13.1",
    "graphql": "^15.8.0",
    "io-ts": "^2.2.20",
    "io-ts-types": "^0.5.19",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/typed-document-node": "^3.0.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@graphql-typed-document-node/apollo-client-2": "^1.0.0",
    "@n1ru4l/graphql-live-query": "^0.10.0",
    "@sudoplatform/sudo-api-client": "^14.0.0",
    "@sudoplatform/sudo-common": "^8.3.1",
    "@sudoplatform/sudo-entitlements": "^16.0.0",
    "@sudoplatform/sudo-user": "^13.0.0",
    "@types/firefox-webext-browser": "^111.0.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.191",
    "@types/node": "^16.18.14",
    "@types/node-localstorage": "^1.3.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "aws-appsync-subscription-link": "^2.4.2",
    "concurrently": "^7.6.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tree-shaking": "^1.10.0",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "node-localstorage": "^2.2.1",
    "prettier": "^2.8.4",
    "sync-fetch": "^0.4.2",
    "ts-jest": "^27.1.5",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.26",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "**/apollo-cache-inmemory": "^1.6.6",
    "apollo-link": "^1.2.14",
    "node-fetch": "^2.6.9",
    "undici": "^5.19.1"
  },
  "peerDependencies": {
    "@sudoplatform/sudo-api-client": "^14.0.0",
    "@sudoplatform/sudo-common": "^8.3.1",
    "@sudoplatform/sudo-user": "^13.0.0"
  }
}
