  "scripts": {
    "clean": "rm -rf ./lib",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint \"{bin,test,src}/**/*.{ts,tsx}\"",
    "lint:prettier": "prettier -l \"{bin,test,src}/**/*.{js,json,jsx,ts,tsx}\" --write ",
    "test": "yarn unit-test",
    "unit-test": "/bin/bash -c 'jest test/unit'",
    "integration-test": "/bin/bash -c 'jest --runInBand test/integration' ",
    "build": "yarn clean && yarn build:gen && yarn build:transpile && yarn build:verify && yarn build:declarations && yarn build:docs",
    "build:gen": "graphql-codegen -c codegen.yml",
    "build:verify": "yarn audit && yarn lint",
    "build:transpile": "babel src --out-dir lib --extensions '.ts'",
    "build:declarations": "tsc --emitDeclarationOnly",
    "build:docs": "typedoc --mode modules --excludePrivate --out docs src/index.ts"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "docs",
    "typings"
  ],
  "browser": {
    "./lib/runtimes/node/node-crypto.js": "./lib/runtimes/browser/browser-crypto.js"
  },
